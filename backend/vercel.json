{
   "version": 2,
   "builds": [
     {
       "src": "api/index.js",
       "use": "@vercel/node"
     }
   ],
   "routes": [
     {
       "src": "/(.*)",
       "dest": "api/index.js"
   "functions": {
     "api/index.js": {
       "runtime": "nodejs18.x"
     }
   ],
   "installCommand": "npm install --legacy-peer-deps"
   },
   "builds": [
     { "src": "api/index.js", "use": "@vercel/node" }
   ]
 }
